{
  "name": "bibi",
  "version": "1.2.0",
  "author": {
    "name": "Satoru Matsushima"
  },
  "description": "Bibi | EPUB Reader on your website.",
  "keywords": [
    "EPUB",
    "Reading System",
    "Viewer",
    "Books-in-Browsers"
  ],
  "license": "MIT",
  "homepage": "https://bibi.epub.link",
  "repository": {
    "type": "git",
    "url": "git://github.com/satorumurmur/bibi.git"
  },
  "bugs": "https://github.com/satorumurmur/bibi/issues",
  "readmeFilename": "README.md",
  "scripts": {
    "postinstall": "",
    "develop": "gulp reset && webpack --progress --config-name development",
    "develop+watch": "gulp reset && webpack --progress --config-name development --watch",
    "develop@wbck": "gulp reset --wbck && webpack --progress --config-name development@wbck",
    "develop@wbck+watch": "gulp reset --wbck && webpack --progress --config-name development@wbck --watch",
    "build": "gulp reset && webpack --progress --config-name production",
    "build+watch": "gulp reset && webpack --progress --config-name production --watch",
    "build@wbck": "gulp reset --wbck && webpack --progress --config-name production@wbck",
    "build@wbck+watch": "gulp reset --wbck && webpack --progress --config-name production@wbck --watch",
    "pack": "gulp reset --wbck --pack && webpack --progress --config-name pack",
    "pack@wbck": "gulp reset --wbck --pack && webpack --progress --config-name pack@wbck",
    "make:dress-template": "gulp make:dress-template",
    "make:package": "npm run pack && gulp make:package --pack && gulp clean --pack",
    "make:package@wbck": "npm run pack@wbck && gulp make:package --pack && gulp make:package --wbck --pack && gulp clean --wbck --pack",
    "serve": "browser-sync start --config bs-config.js",
    "demo": "npm run build@wbck && npm run serve",
    "start": "npm run develop+watch"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.4",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.7",
    "cssnano": "^5.0.6",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^12.1.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "dependencies": {
    "bibi-zip-loader": "^0.9.0",
    "classlist-polyfill": "^1.2.0",
    "current-script-polyfill": "^1.0.0",
    "custom-event-polyfill": "^1.0.7",
    "dompurify": "^2.3.3",
    "es6-object-assign": "^1.1.0",
    "intersection-observer": "^0.12.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.6.0",
    "jszip-utils": "^0.1.0",
    "material-icons": "^0.7.6",
    "native-promise-only": "^0.8.1",
    "nosleep.js": "^0.12.0",
    "polyfill-array-includes": "^2.0.0",
    "sml.js": "^1.0.30",
    "string.padstart": "^0.1.1",
    "text-encoding-utf-8": "^1.0.2",
    "url-polyfill": "^1.1.12"
  },
  "browserslist": [
    "defaults",
    "ie 11"
  ]
}
